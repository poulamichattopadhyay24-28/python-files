def interpolation_search(arr, key):
    low, high = 0, len(arr) - 1

    while low <= high and key >= arr[low] and key <= arr[high]:
        if low == high:
            if arr[low] == key:
                return low
            return -1

        
        pos = low + ((high - low) * (key - arr[low])) // (arr[high] - arr[low])

        if arr[pos] == key:
            return pos
        elif arr[pos] < key:
            low = pos + 1
        else:
            high = pos - 1
    return -1



n = int(input("Enter number of elements: "))
arr = []

print("Enter elements in sorted order:")
for i in range(n):
    arr.append(int(input()))

key = int(input("Enter element to search: "))

result = interpolation_search(arr, key)

if result != -1:
    print(f"Element {key} found at position {result+1}")
else:
    print(f"Element {key} not found in the array.")
